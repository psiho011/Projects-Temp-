   "source": [
    "import QuantLib as ql\n",
    "import matplotlib.pyplot as plt\n",
    "import numpy as np\n",
    "\n",
    "def main():\n",
    "    while True:\n",
    "        try:\n",
    "            ##These establish the inputs we want the user to enter\n",
    "            principal = float(input(\"Enter loan amount ($): \"))\n",
    "            annual_rate = float(input(\"Enter annual interest rate (%): \"))\n",
    "            years = int(input(\"Enter loan term (years): \"))\n",
    "            ###This calls what we returned in the mortgage_schedule value-returning function\n",
    "            payment, balances = mortgage_schedule(principal, annual_rate, years)\n",
    "\n",
    "            print(\"Monthly payment: ${:.2f}\".format(payment))\n",
    "\n",
    "            #This creates a line graph that shows the amount owed throughout the life of the loan\n",
    "            months = np.arange(1, years*12 + 1)\n",
    "            plt.plot(months, balances)\n",
    "            plt.title(\"Home Loan Balance Over Time (${:,.2f}, {}% for {} years)\".format(principal, annual_rate, years))\n",
    "            plt.xlabel(\"Month\")\n",
    "            plt.ylabel(\"Remaining Balance ($)\")\n",
    "            plt.grid(True)\n",
    "            ###This shows the plots on the graph every 5 years to give an idea of how much is owed every 5 years.\n",
    "            ###Helps make the graph easier to follow\n",
    "            for m in range(0, years*12, 60):\n",
    "                plt.scatter(m+1, balances[m], color='red')\n",
    "                plt.text(m+1, balances[m], \"${:,.0f}\".format(balances[m]), fontsize=8, ha='left', va='bottom')\n",
    "            plt.show()\n",
    "        except:\n",
    "            print(\"Invalid input, please try again.\")\n",
    "\n",
    "        if input(\"Try another loan? (y/n): \").lower() != \"y\":\n",
    "            break\n",
    "\n",
    "def mortgage_schedule(principal, annual_rate, years):\n",
    "    monthly_rate = annual_rate / 100 / 12\n",
    "    n_payments = years * 12\n",
    "    #Monthly payment formula\n",
    "    payment = principal * (monthly_rate * (1 + monthly_rate)**n_payments) / ((1 + monthly_rate)**n_payments - 1)\n",
    "    balances = []\n",
    "    balance = principal\n",
    "    for i in range(n_payments):\n",
    "        interest = balance * monthly_rate\n",
    "        principal_paid = payment - interest\n",
    "        balance -= principal_paid\n",
    "        balances.append(balance)\n",
    "    return payment, balances\n",
    "\n",
    "main()\n"
   ]
  },
